\"tm-header\" v-bind:class=\"{ show:slide}\" id=\"tm-header\">\n    <div class=\"tm-header-wrapper\" >\n      <button @click=\"slider\"  class=\"navbar-toggler\" type=\"button\" aria-label=\"Toggle navigation\" >\n        <i class=\"fas fa-bars\" ></i>\n      </button>\n      <div class=\"tm-site-header\">\n        <div class=\"mb-3 mx-auto tm-site-logo\" ><i style=\"text-shadow: 0 0 15px;\">S</i></div>\n        <h1 class=\"text-center\" style=\"color: #008c8c; font-weight: bold; text-shadow: 0 0 15px\">Smart Shelf</h1>\n      </div>\n      <nav class=\"tm-nav\"  id=\"tm-nav\">\n        <ul>\n          <li class=\"tm-nav-item active\" ><a @click=\"goMain\" href=\"#\" class=\"tm-nav-link\" style=\"color: #008c8c\">\n            <i class=\"fas fa-home\" ></i>\n            Мои полки\n          </a></li>\n\n          <li v-if=\"auth\" class=\"tm-nav-item\"><a @click=\"goMenu\" href=\"#\" class=\"tm-nav-link\">\n            <i class=\"fas fa-users\"></i>\n            Мое меню\n          </a></li>\n\n\n          <li class=\"tm-nav-item\" v-if=\"!auth\"><a @click=\"goLogin\" href=\"#\" class=\"tm-nav-link\" >\n            <i  class=\"fas fa-pen\"></i>\n            Войти</a>\n          </li>\n          <li class=\"tm-nav-item\" v-else><a @click=\"goLogout\" href=\"#\" class=\"tm-nav-link\" >\n            <i  class=\"fas fa-pen\"></i>\n            Выйти</a>\n          </li>\n\n\n        </ul>\n      </nav>\n\n      <p class=\"tm-mb-80 pr-5 text-white\">\n      </p>\n    </div>\n  </header>\n</template>\n\n<script>\nexport default {\n  name: \"Nav\",\n  data(){\n    return{\n      slide:false,\n    }\n  },\n  computed: {\n\n    auth() {\n      if (localStorage.getItem(\"auth_token\")) {\n        return true\n      }\n    }\n  },\n  methods: {\n    goLogin() {\n      this.$router.push({name: \"login\"});\n      this.slide = false\n    },\n    goLogout() {\n      localStorage.removeItem(\"auth_token\")\n      this.slide = false\n      window.location = '/'\n    },\n    goMain() {\n      this.$router.push({name: \"Home\"})\n      this.slide = false\n    },\n    goMenu() {\n      this.$router.push({name: \"category\"})\n      this.slide = false\n    },\n    slider(){\n      if (this.slide === true){\n        this.slide = false\n      }\n      else{\n        this.slide = true\n      }\n    }\n  }\n}\n\n</script>\n\n\n\n"],"sourceRoot":"src/components"}]}