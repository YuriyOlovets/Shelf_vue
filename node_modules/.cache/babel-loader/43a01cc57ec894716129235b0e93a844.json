A,EAcA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,MAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,IAAA,EAAA,IAAA;AAAA,UAAA,IAAA,EAAA;AAAA;AAAA,OAAA;AACA;AAhBA,GAVA;AA4BA,EAAA,OA5BA,qBA4BA;AACA,SAAA,WAAA;AACA;AA9BA,CAAA","sourcesContent":["<template>\n  <div class=\"container-fluid\">\n    <main class=\"tm-main\">\n      <!-- Search form -->\n      <div class=\"row tm-row\">\n        <div class=\"col-12\" >\n          <h2 class=\"main\">{{name}}</h2>\n        </div>\n      </div>\n        <h3>Выбрать дату:</h3>\n        <input v-model=\"day\"  type=\"date\" id=\"start\" name=\"date\" >\n        <button @click=\"goDate(day,id)\" type=\"submit\" class=\"myButton\">Смотреть</button>\n      <br>\n      <button @click=\"goDayList(id,name)\" class=\"myButton\">Просмотр по дням</button>\n      <br><br>\n\n      <div  class=\"row tm-row\">\n        <article class=\"col-12 col-md-6 tm-post\">\n          <table>\n            <thead >\n            <tr>\n              <th>Вес</th>\n              <th>Дата</th>\n            </tr>\n            </thead>\n\n            <tbody>\n            <tr v-for=\"cell in History\" :key=\"cell.id\">\n              <td>{{cell.weight}}</td>\n              <td>{{cell.update_time}}</td>\n            </tr>\n\n            </tbody>\n          </table>\n        </article>\n\n      </div>\n\n    </main>\n  </div>\n\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"History\",\n  props: ['id','name'],\n  data() {\n    return {\n      History: [],\n      day:''\n    }\n  },\n\n  methods: {\n    async LoadHistory() {\n      this.History = await axios({\n        method: 'get',\n        url: this.$store.getters.getServerUrl +'/history/' + this.id + '/',\n        headers: {\n          Authorization: 'Token ' + localStorage.getItem(\"auth_token\")\n        }\n      }).then(response => response.data);\n      console.log(this.History)\n    },\n    goDate(day,cell){\n      this.$router.push({name:'date', params:{day:day,cell:cell}})\n    },\n    goDayList(list){\n      this.$router.push({name:'list', params:{list:list,name:name}})\n    }\n  },\n  created() {\n    this.LoadHistory()\n  },\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/views"}]}