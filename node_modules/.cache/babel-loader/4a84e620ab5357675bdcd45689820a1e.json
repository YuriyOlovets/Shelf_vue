\" class=\"styled-table\">\n    <thead>\n    <tr>\n      <th></th>\n      <th>Продукт</th>\n      <th>Вага</th>\n    </tr>\n    </thead>\n    <tbody v-for=\"prod in Delivery\" :key=\"prod.id\" >\n    <tr class=\"active-row\">\n      <td><img class=\"photo\" :src=\"prod.product[0].product_image\"></td>\n      <td>{{prod.product[0].product_name_}}</td>\n      <td>1000г<br></td>\n    </tr>\n    </tbody>\n  </table>\n    <table v-else  style=\"width: 350px\" class=\"styled-table\">\n      <thead>\n      <tr>\n        <th></th>\n        <th>Product</th>\n        <th>Weight</th>\n      </tr>\n      </thead>\n      <tbody v-for=\"prod in Delivery\" :key=\"prod.id\" >\n      <tr class=\"active-row\">\n        <td><img class=\"photo\" :src=\"prod.product[0].product_image\"></td>\n        <td>{{prod.product[0].product_name_en}}</td>\n        <td>1000g<br></td>\n      </tr>\n      </tbody>\n    </table>\n\n    <button v-if=\"Language\" style=\"width: 300px\" class=\"myButton\">Замовити</button>\n    <button v-else style=\"width: 300px\" class=\"myButton\">To order</button>\n  </main>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"Delivery\",\n  data(){\n    return{\n      Delivery: [],\n      Delivery_list:[],\n      i:'',\n\n    }\n  },\n  computed: {\n    Language(){\n      if (localStorage.getItem('language')==='en')\n        return false\n      else\n        return true\n    }\n\n  },\n  methods:{\n    async LoadDelivery(){\n      this.Delivery = await axios({\n        method: 'get',\n        url: this.$store.getters.getServerUrl +'/delivery/',\n        headers: {\n          Authorization: 'Token ' + localStorage.getItem(\"auth_token\")\n        }\n      }).then(response => response.data);\n\n    },\n  },\n  created() {\n    this.LoadDelivery()\n\n  }\n}\n</script>\n\n<style scoped>\n.styled-table {\n  border-collapse: collapse;\n  margin: 25px 0;\n  font-size: 0.9em;\n  font-family: sans-serif;\n  min-width: 40px;\n  box-shadow: 0 0 