,EAkBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA;AACA,kBAAA,MAAA,EAAA,KADA;AAEA,kBAAA,GAAA,EAAA,MAAA,CAAA,MAAA,CAAA,OAAA,CAAA,YAAA,GAAA,SAAA,GAAA;AAFA,iBAAA,CADA;;AAAA;AAKA,gBAAA,QAAA,CAAA,MAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KAxBA;AAyBA,IAAA,UAzBA,sBAyBA,GAzBA,EAyBA;AACA,aAAA,GAAA,GAAA,IAAA,IAAA,CAAA,GAAA,EAAA,cAAA,EAAA,GAAA,EAAA;AACA,KA3BA;AA4BA,IAAA,WA5BA,yBA4BA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,YAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,YAAA,CAAA,OAAA,CAAA,YAAA,CAAA;AACA,OAFA,MAGA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA;AACA;AAnCA,GAtBA;AA4DA,EAAA,OA5DA,qBA4DA;AACA,SAAA,WAAA;AACA,SAAA,SAAA;AAGA;AAjEA,CAAA","sourcesContent":["<template>\n\n      <main class=\"tm-main \" >\n        <!-- Search form -->\n        <div class=\"row tm-row\">\n          <div v-if=\"TrueShelf\" class=\"col-12\" >\n            <h2 class=\"main\">My product:</h2>\n          </div>\n            <Cells v-if=\"TrueShelf\"> </Cells>\n\n            <a href=\"#\" v-if=\"!TrueShelf\"> <button @click=\"goAdd\" class=\"myButton\">Добавить новую полку</button></a>\n\n<!--                    <div v-if=\"TrueShelf\" class=\"col-12\" >-->\n<!--                      <h2 class=\"main\">Мои продукты:</h2>-->\n<!--                    </div>-->\n                  </div>\n                  <div class=\"col-4 row tm-row\" >\n                    <article class=\"col-12   tm-post\">\n                      <div>\n                      <table class=\"styled-table\"  v-for=\"shelf in Shelf\" :key=\"shelf.id\">\n\n                        <thead>\n                        <tr class=\"active-row\">\n                          <th>ID полки</th>\n                          <th>Последнее соединение</th>\n                          <th>Отвязать</th>\n\n                        </tr>\n                        </thead>\n                        <tbody>\n                        <tr>\n                          <td><a>{{shelf.ident}}</a></td>\n                          <td>{{formatDate(shelf.last_connect)}}</td>\n                          <td ><a href=\"#\" style=\"color:red\"><button @click=\"Delete(shelf.ident)\" class=\"myButton\">Отвязать полку</button></a></td>\n\n\n                        </tr>\n\n                        </tbody>\n                      </table>\n                      </div>\n                    </article>\n        </div>\n\n      </main>\n\n\n</template>\n\n<script>\nimport Cells from \"../components/Cells\";\nimport axios from 'axios'\n\nexport default {\n  name: 'Home',\n  components: {Cells},\n  data(){\n    return{\n      Shelf : [],\n      Cells:[],\n      cell_id:null,\n    }\n  },\n\n  computed: {\n    TrueShelf() {\n      if (this.Shelf.length > 0) {\n        return true\n      }\n      else{\n        return false\n      }\n    }\n  },\n\n  methods : {\n    async loadShelf()\n    {\n    this.Shelf = await\n        axios({\n        method: 'get',\n        url: 'https://smart-shelf-bbc6g.ondigitalocean.app/api/v1/shelf/',\n        headers: {\n          Authorization: 'Token '+ localStorage.getItem(\"auth_token\")\n        }\n      })\n          .then(response => response.data );\n    },\n    goAdd() {\n      this.$router.push({name: \"add\"})\n    },\n\n    async Delete(ident)\n    {\n      await axios({\n            method: 'get',\n            url: this.$store.getters.getServerUrl + 'delete/'+ ident,\n          });\n      location.reload()\n    },\n    formatDate(str) {\n      return str ? new Date(str).toLocaleString() : \"\"\n    },\n    login_check() {\n      if (localStorage.getItem(\"auth_token\")) {\n        console.log(localStorage.getItem(\"auth_token\"))\n      }\n      else {\n        this.$router.push({name: \"login\"})\n      }\n    },\n\n  },\n  created() {\n    this.login_check()\n    this.loadShelf()\n\n\n  }\n}\n\n</script>\n\n<style scoped>\n\n.styled-table {\n  border-collapse: collapse;\n  margin: 25px 0;\n  font-size: 0.9em;\n  font-family: sans-serif;\n  min-width: 40px;\n  box-shadow: 0 0 20px rgba(0, 0, 0, 0.15);\n}\n\n.styled-table thead tr {\n  background-color: #008c8c;\n  color: #ffffff;\n  text-align: left;\n}\n\n.styled-table th,\n.styled-table td {\n  padding: 12px 15px;\n}\n\n.styled-table tbody tr {\n  border-bottom: 1px solid #dddddd;\n}\n\n.styled-table tbody tr:nth-of-type(even) {\n  background-color: #ffcc99;\n}\n\n.styled-table tbody tr:last-of-type {\n  border-bottom: 2px solid #009879;\n}\n.styled-table tbody tr.active-row {\n  font-weight: bold;\n  color: #ffcc99;\n}\n\n</style>"],"sourceRoot":"src/views"}]}