\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"list\",\n  props: ['list'],\n  data(){\n    return{\n      List:[]\n    }\n  },\n  methods:{\n    async LoadHistoryList() {\n      this.List = await axios({\n        method: 'get',\n        url: 'http://127.0.0.1:8000/api/v1/day_list/' + this.list + '/',\n        headers: {\n          Authorization: 'Token ' + localStorage.getItem(\"auth_token\")\n        }\n      }).then(response => response.data);\n    },\n  },\n  created() {\n    this.LoadHistoryList()\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]}]}