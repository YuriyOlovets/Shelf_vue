  </div>\n\n    </main>\n  </div>\n\n\n  <div v-else-if=\"!Language\" class=\"container-fluid\">\n    <main class=\"tm-main\">\n      <!-- Search form -->\n      <div class=\"row tm-row\">\n        <div class=\"col-12\" >\n          <h2 class=\"main\">{{day}}</h2>\n        </div>\n      </div>\n      <br>\n      <br><br>\n\n      <div  class=\"row tm-row\">\n        <article class=\"col-12 col-md-6 tm-post\">\n          <table>\n            <thead >\n            <tr>\n              <th>Weight</th>\n              <th>Date</th>\n            </tr>\n            </thead>\n\n            <tbody>\n            <tr v-for=\"cell in Day\" :key=\"cell.id\">\n              <td>{{cell.weight}}</td>\n              <td>{{cell.update_time}}</td>\n            </tr>\n\n            </tbody>\n          </table>\n        </article>\n\n      </div>\n\n    </main>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"date\",\n  props: ['day','cell'],\n  data(){\n  return{\n    Day:[]\n  }\n  },\n  computed: {\n    Language(){\n      if (localStorage.getItem('language')==='en')\n        return false\n      else\n        return true\n    }\n  },\n  methods:{\n    async LoadDay() {\n       this.Day = await axios({\n        method: 'post',\n        url: this.$store.getters.getServerUrl +'/date/' + this.cell + '/',\n        data:{\n          date:this.day\n        }\n      }).then(response => response.data);\n\n\n    },\n  },\n\n  created() {\n      this.LoadDay()\n  }\n\n}\n</script>\n\n<style scoped>\n\n</style>"]}]}