    </table>\n\n\n\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Change_name from \"../views/Change_name\";\n\nexport default {\n  name: \"Cells\",\n  data(){\n    return{\n      Cells:[],\n      cell_id:null,\n    }\n  },\n  methods:{\n    async LoadIdent(){\n       this.cell_id = await axios({\n        method: 'get',\n        url: 'http://127.0.0.1:8000/api/v1/shelf',\n        headers: {\n          Authorization: 'Token ' + localStorage.getItem(\"auth_token\")\n        }\n      }).then(response => response.data[0]['ident']);\n      this.Cells = await axios({\n        method: 'get',\n        url: 'http://127.0.0.1:8000/api/v1/shelf/'+this.cell_id,\n        headers: {\n          Authorization: 'Token ' + localStorage.getItem(\"auth_token\")\n        }\n      }).then(response => response.data);\n\n    },\n    goChange(ident,name){\n      this.$router.push({name:'change', params:{ident:ident,name:name}})\n    },\n    goHistory(id){\n      this.$router.push({name:'history', params:{id:id}})\n    }\n    },\n  created() {\n    this.LoadIdent();\n\n\n  }\n\n}\n\n</script>\n\n<style scoped>\n\n</style>"]}]}